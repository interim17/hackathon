{
    "name": "Tim and Joe's Extension Adventure",
    "description": "Build an Extension!",
    "version": "2",
    "manifest_version": 3,
    "web_accessible_resources" : [{
      "resources": [ "images/*", "http://clipart-library.com/images/piqd4jMyT.gif"],
       "matches": ["http://*/*", "https://*/*"]
      }],
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "tabs", "activeTab", "scripting", "extension"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
      },
      "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
      }
    }
    
  }

 